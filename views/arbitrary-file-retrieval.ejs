        <!-- Content Header (Page header) -->
        <section class="content-header">
          <div class="container-fluid">
            <div class="row mb-2">
              <div class="col-sm-6">
                <h1>Arbitrary File Retrieval</h1>
                <p>
                  Change the <b>filename</b> parameter value in the url to
                  <b>../app.js</b> <br>
                  This reads this particular project's <b>app.js</b> file, this vulnerability can be used to get to the root of a server and exploit with the files present there.
                </p>
              </div>
            </div>
          </div>
          <!-- /.container-fluid -->
        </section>

        <!-- Main content -->
        <section class="content">
          <div class="row">
